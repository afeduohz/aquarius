# md5 #

(let x = (crypto/md5: "this is a test file 中文 d."))
(assert: (x == "9528ab7e7a2080319d5b486cf82a085e") "test (crypto/md5:)")

# sha1 #

(let x = (crypto/sha1: "this is a test file 中文 d."))
(assert: (x == "56f4bc91e8bd8af76fa1238cc63229df20fa5d4d") "test (crypto/sha1:)")

# sha256 #

(let x = (crypto/sha256: "this is a test file 中文 d."))
(assert: (x == "732872b73a2220d00bc9ecb1820da5cd7d09f3efc387800d5acc8daa5b00231c") "test (crypto/sha256:)")

(puts: "crypto/* checkpoints all passed.")