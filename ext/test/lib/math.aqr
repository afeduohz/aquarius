# >>> module math #

# == #
(assert:(math == math) "test (math == math)")

# != #
(assert:(math != 1) "test (math != 1)")
(assert:(math != nil) "test (math != nil)")

# to_s #
(assert:((math to_s) == "<module math>") "test (math to_s)")

# to_b #
(assert:((math to_b) == true) "test (math to_b)")

# type #
(assert:((math type) == "lambda") "test (math type)")

# ceil #
(assert:((math ceil 1.6) == 2) "test (math ceil 1.6)")

# floor #
(assert:((math floor 1.6) == 1) "test (math floor 1.6)")

# sin #
(assert:((math sin 0.0) == 0) "test (math sin 0.0)")

# asin #
(assert:((math asin 0.0) == 0) "test (math asin 0.0)")

# sinh #
(assert:((math sinh 0.0) == 0) "test (math sinh 0.0)")

# asinh #
(assert:((math asinh 0.0) == 0) "test (math asinh 0.0)")

# cos #
(assert:((math cos 0.0) == 1.0) "test (math cos 0.0)")

