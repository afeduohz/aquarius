#base32#

"decode"

(let x = (encoding/base32 decode "GEZDGNBVGY======"))
(assert: (x == "123456") "base32 decode,1")

"encode"

(let x = (encoding/base32 encode "123456"))
(assert: (x == "GEZDGNBVGY======") "base32 encode,1")


#base64#

"decode"

(let x = (encoding/base64 decode "MTIzNDU2"))
(assert: (x == "123456") "base32 decode,1")

"encode"

(let x = (encoding/base64 encode "123456"))
(assert: (x == "MTIzNDU2") "base32 encode,1")